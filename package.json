  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "PORT=3004 react-scripts start",
    "start-win": "set PORT=3004 && react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.development react-scripts build",
    "build:uat": "env-cmd -f .env.test react-scripts build",
    "build:prod": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "test:last-commit-coverage": "react-scripts test -i --coverage --reporters=jest-junit",
    "test:coverage": "react-scripts test --coverage --reporters=jest-junit",
    "test:full-coverage": "react-scripts test --coverage --watchAll=false --reporters=jest-junit",
    "eject": "react-scripts eject",
    "lint-autofix": "eslint /src -c .eslintrc.js --ext .js,.ts,.tsx --fix",
    "lint": "eslint /src -c .eslintrc.js --ext .js,.ts,.tsx ",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\"",
    "serve": "npx serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
